include "macos.phtn"

macro STDIN 0 end
macro STDOUT 1 end
macro STDERR 2 end

macro write STDOUT SYS_WRITE syscall3 end
macro writeln write "\n" write end
macro exit SYS_EXIT syscall1 end

macro inc64
  dup ,64 1 + .64
end

macro strlen
   dup
   while dup , 0 != do 1 + end
   swap -
end

macro argv[n]
    8 * argv + ,64 int->ptr
end

macro dup2 over over end
macro drop2 drop drop end

macro % dup2 / * - end